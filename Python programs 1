variable=str(raw_input('ENTER YOUR NAME\n'))
gender=str(raw_input("YOU ARE A GIRL OR A BOY\n"))
while 1==1:
    a=str(raw_input('A for ....\n'))
    point=4
    if a=="APPLE" or "AEROPLANE" or "ANT" or "ANIMAL" or  "ANACONDA":
        point+=1
        print 'YOUR SCORE IS', point
        print variable, " IS A GOOD ", gender
        break
    else:
        print 'YOU SHOULD STUDY WELl'
        point-=1

        
while 1==1:
    a=str(raw_input('B for ....\n'))
